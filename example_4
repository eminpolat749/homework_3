package csd;

class App {
	public static void main(String [] args)
	{
		IsFactorianTest.run();		
	}
}

class IsFactorianTest {
	public static void run()
	{
		java.util.Scanner kb = new java.util.Scanner(System.in);
		
		for (int i = 1; i < 100000; ++i) 
			if (NumberUtil.isFactorian(i))
				System.out.println(i);		
		
		System.out.println("Döngü sonu");
	}
}

class NumberUtil {
	public static boolean isFactorian(int val)
	{
		int digit = countDigits(val);
		int total = 0, temp = val;
		
		for (int i = 0; i < digit; ++i) {
			total += factorial(val % 10);
			val /= 10;
		}
		
		return total == temp; 
	}
	
	public static long factorial(int val)
	{
		long result = 1;
		
		for (int i = 2; i <= val; ++i)
			result *= i;
		
		return result;
	}	
	public static int countDigits(int val)
	{		
		int cnt = 0;
		
		do {
			++cnt;
			val /= 10;
		} while (val != 0);
		
		return cnt;
	}
}
